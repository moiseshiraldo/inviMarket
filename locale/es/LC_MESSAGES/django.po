#: templates/glossary.html:7
msgid ""
"\n"
"  <h1>Glossary</h1>\n"
"\n"
"  <section id=\"C\" class=\"infotext\">\n"
"    <h2>C</h2>\n"
"    <ul>\n"
"     <li>\n"
"       <h4 class=\"faq-header\">Complaint</h4>\n"
